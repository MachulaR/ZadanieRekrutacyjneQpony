<table class="table table-striped table-sm text-center">
    <thead>
    <tr>
        <th>#</th>
        <th>Ilość znaków w ciągu</th>
        <th>Największa wartość w ciagu</th>
    </tr>
    </thead>
    <tbody>
    {% for key, numbers in data %}
        <tr>
            <td>{{ key +1}}</td>
            <td>{{ numbers.entryNumber }}</td>
            <td>{{ numbers.biggestValue }} </td>
        </tr>
    {% endfor %}
    </tbody>
</table>
